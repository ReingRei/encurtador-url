{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/encurtador-url/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/encurtador-url/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/encurtador-url",
  "projects": {
    "autenticacao": {
      "type": "application",
      "root": "apps/autenticacao",
      "entryFile": "main",
      "sourceRoot": "apps/autenticacao/src",
      "compilerOptions": {
        "tsConfigPath": "apps/autenticacao/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "core-config": {
      "type": "library",
      "root": "libs/core-config",
      "entryFile": "index",
      "sourceRoot": "libs/core-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core-config/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "encurtador-url": {
      "type": "application",
      "root": "apps/encurtador-url",
      "entryFile": "main",
      "sourceRoot": "apps/encurtador-url/src",
      "compilerOptions": {
        "tsConfigPath": "apps/encurtador-url/tsconfig.app.json"
      }
    }
  }
}